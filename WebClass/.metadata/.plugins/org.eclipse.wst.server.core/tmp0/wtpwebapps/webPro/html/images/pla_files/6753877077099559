Y(event);\\n        if (this.vidPlaying) { this.videoEl.pause(); }\\n    }\\n\\n    this.handleMove = function (event) {\\n        if (!this.interactionsStarted) return;\\n        this.xy = toXY(event);\\n        this.dxy = { x: this.xy.x - this.xy0.x, y: this.xy.y - this.xy0.y };\\n\\n        if (this.prevented === 0) {\\n            if (Math.abs(this.dxy.y) - Math.abs(this.dxy.x) > 5 && Math.abs(this.dxy.y) + Math.abs(this.dxy.x) > 15) {  // moved mouse more than 15px + check which direction\\n                this.prevented = false;\\n            } else if (Math.abs(this.dxy.x) + Math.abs(this.dxy.y) > 15) {\\n                this.prevented = true;\\n            }\\n        }\\n\\n        if (!this.prevented) return;\\n        this.preventedFlag = true;\\n        event.preventDefault();\\n\\n        if (!this.firstInteraction) {\\n            clearTimeout(this.startAuto);\\n            clearInterval(this.autoInterval);\\n\\n            this.firstInteraction = true;\\n            if (this.previewImg) this.previewImg.hide();\\n            bntTracking.trackEvent(\\\"#First User Interaction\\\", null);\\n        }\\n\\n        this.percent = this.dxy.x / this.containerWidth;\\n\\n        let next;\\n\\n        if (this.percent > 0) {\\n            next = this.mod(this.activeIndex - 1, this.slides.length)\\n        }\\n        else {\\n            next = this.mod(this.activeIndex + 1, this.slides.length)\\n        }\\n\\n        if (next != this.nextIndex) {\\n\\n            if (this.nextIndex != null) {\\n                this.slides[this.nextIndex].htmlElement.style.opacity = 0;\\n            }\\n            this.nextIndex = next;\\n            this.slides[this.nextIndex].htmlElement.style.opacity = 1;\\n            if (this.pairity) this.slides[this.nextIndex].htmlElement.style.transform = this.rotateAxis + '(180deg) translateZ(3px)';\\n            else this.slides[this.nextIndex].htmlElement.style.transform = this.rotateAxis + '(0deg) translateZ(3px)';\\n        }\\