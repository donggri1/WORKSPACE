this.speed || config.auto == null) ? config.auto : this.speed;\n    this.startAutoTime = config.startAutoAfter ? config.startAutoAfter : 0;\n    this.percent = 0;\n    this.spinLoop;\n\n    if (config.rotateAxis == 'x' && this.onscroll) {\n        this.rotateAxis = 'rotateX';\n    } else {\n        this.rotateAxis = 'rotateY';\n    }\n\n    this.interactionsStarted = false;\n    this.activeIndex = 0;\n    this.lastRotation = 0;\n    this.nextIndex = null;\n    this.pairity = true;\n    this.firstInteraction = false;\n    this.spam = false\n\n    this.videoEl = null;\n    this.videoidx = null;\n    this.vidPlaying = false;\n    this.trackedSlides = [0];\n\n    this.slides = this.container.eos.map(function (slide) { return slide.element; });\n\n    this.screen.onshow.addObserver(function () {\n        this.init();\n    }.bind(this))\n\n    this.screen.onhide.addObserver(function () {\n        this.stopLoop();\n    }.bind(this));\n\n    this.init = function () {\n        this.setupContainerPosition();\n        this.setupSlidesPosition();\n        this.checkVideo();\n\n        bntTracking.trackEvent(\"#Slide \" + (this.activeIndex + 1) + ': in view', null);\n\n        if (this.onscroll) {\n            this.loop();\n            this.container.htmlElement.classList.remove(this.animation);\n\n        } else {\n            this.addInteractions();\n\n            if (this.autoTime) {\n                this.startAuto();\n            }\n        }\n    }\n\n    this.startAuto = function () {\n        this.startAuto = setTimeout(function () {\n\n            setTimeout(function () {\n                this.container.htmlElement.classList.remove(this.animation);\n            }.bind(this), this.autoTime * 1000 - 100)\n\n            this.autoInterval = setInterval(function () {\n                this.autoNext();\n            }.bind(this), this.autoTime * 1000)\n\n        }.bind(this), this.startAutoTime * 1000)\n    }\n\n    this.setupContainerPosition = function () {\n        this.rotator = this.container.htmlElement.childNodes[0].childNodes[0];\n        this.rotator.style.transformStyle = 'preserve-3d';\n        this.rotator.style.transition = 'transform ' + this.speed + 's';\n        this.container.htmlElement.childNodes[0].style.perspective = this.perspective + 'px';\n\n        this.container.htmlElement.classList.add(this.animation);\n\n        this.containerWidth = this.container.htmlElement.offsetWidth;\n        this.containerLeft = this.container.htmlElement.offsetLeft;\n\n        this.trueElementLeft = this.slides[0].eos[this.slides[0].eos.length - 1].element.htmlElement.offsetLeft + this.slides[0].htmlElement.offsetLeft;\n        this.trueElementWidth = this.slides[0].eos[this.slides[0].eos.length - 1].element.htmlElement.offsetWidth;\n\n        this.outlineOffset = this.containerLeft + this.trueElementLeft;\n        if (this.containerWidth > this.trueElementWidth) {\n            this.rotator.style.width = bnt.px(this.trueElementWidth);\n            this.container.htmlElement.style.left = bnt.px(this.outlineOffset);\n            this.containerWidth = this.container.htmlElement.offsetWidth;\n        }\n\n        this.containerHeight = this.container.htmlElement.offsetHeight;\n        this.containerTop = this.container.htmlElement.offsetTop;\n\n        this.trueElementTop = this.slides[0].eos[this.slides[0].eos.length - 1].element.htmlElement.offsetTop + this.slides[0].htmlElement.offsetTop;\n        this.trueElementHeight = this.slides[0].eos[this.slides[0].eos.length - 1].element.htmlElement.offsetHeight;\n\n        this.outlineOffset = this.containerTop + this.trueElementTop;\n        if (this.containerHeight > this.trueElementHeight) {\n            this.rotator.style.height = bnt.px(this.trueElementHeight);\n            this.container.htmlElement.style.top = bnt.px(this.outlineOffset);\n            this.containerHeight = this.container.htmlElement.offsetHeight;\n        }\n    }\n\n\n    this.setupSlidesPosition = function () {\n        this.slides.forEach(function (el, idx) {\n\n            el.htmlElement.style.backfaceVisibility = 'hidden';\n\n            el.eos.forEach(function (slideElement, slideElement_idx) {\n                let elementId = slideElement.element.elementId.toLowerCase()\n                if (elementId.indexOf(\"item\") != -1)\n                    slideElement.element.htmlElement.style.transform = \"translateZ(30px)\";\n                else\n                    slideElement.element.htmlElement.style.transform = \"translateZ(1px)\";\n            })\n\n            if (el.htmlElement.offsetWidth == this.trueElementWidth) {\n                el.htmlElement.style.left = \"0px\";\n            } else {\n                if (el.eos[el.eos.length - 1].element.htmlElement.offsetLeft > 0) {\n                    el.htmlElement.style.left = bnt.px(- el.eos[el.eos.length - 1].element.htmlElement.offsetLeft);\n                    el.htmlElement.style.right = bnt.px(- el.eos[el.eos.length - 1].element.htmlElement.offsetLeft);\n                } else {\n                    el.htmlElement.style.left = \"0px\";\n                }\n            }\n\n            if (el.htmlElement.offsetHeight == this.trueElementHeight) {\n                el.htmlElement.style.top = \"0px\";\n                el.htmlElement.style.bottom = \"0px\";\n            } else {\n                if (el.eos[el.eos.length - 1].element.htmlElement.offsetTop > 0) {\n                    el.htmlElement.style.top = bnt.px(- el.eos[el.eos.length - 1].element.htmlElement.offsetTop);\n                    el.htmlElement.style.bottom = bnt.px(- el.eos[el.eos.length - 1].element.htmlElement.offsetTop);\n                } else {\n                    el.htmlElement.style.top = \"0px\";\n                    el.htmlElement.style.bottom = \"0px\";\n                }\n            }\n\n            if (idx) {\n                el.htmlElement.style.opacity = 0;\n            }\n        }.bind(this))\n    }\n\n    this.checkVideo = function () {\n        for (var idx = 0; idx < this.container.eos.length; idx++) {\n            var vids = this.slides[idx].deepGetEosByType(bnt.Video);\n            if (vids[0]) {\n                this.videoEl = vids[0].element;\n                this.videoidx = idx;\n                if (idx == 0) this.vidPlaying = true;\n                else this.videoEl.pause();\n                break;\n            }\n        }\n    };\n\n    this.handleDown = function (event) {\n        if (this.spam) return;\n        if (this.slides.length <= 1) return;\n        this.rotator.style.transition = '';\n        this.container.htmlElement.classList.remove(this.animation);\n        this.prevented = 0;\n        this.interactionsStarted = true;\n        // bntTracking.trackEvent(\"#User Interaction started\", null);\n        this.xy0 = toXY(event);\n        if (this.vidPlaying) { this.videoEl.pause(); }\n    }\n\n    this.handleMove = function (event) {\n        if (!this.interactionsStarted) return;\n        this.xy = toXY(event);\n        this.dxy = { x: this.xy.x - this.xy0.x, y: this.xy.y - this.xy0.y };\n\n        if (this.prevented === 0) {\n            if (Math.abs(this.dxy.y) - Math.abs(this.dxy.x) > 5 && Math.abs(this.dxy.y) + Math.abs(this.dxy.x) > 15) {  // moved mouse more than 15px + check which direction\n                this.prevented = false;\n            } else if (Math.abs(this.dxy.x) + Math.abs(this.dxy.y) > 15) {\n                this.prevented = true;\n            }\n        }\n\n        if (!this.prevented) return;\n        this.preventedFlag = true;\n        event.preventDefault();\n\n        if (!this.firstInteraction) {\n            clearTimeout(this.startAuto);\n            clearInterval(this.autoInterval);\n\n            this.firstInteraction = true;\n            if (this.previewImg) this.previewImg.hide();\n            bntTracking.trackEvent(\"#First User Interaction\", null);\n        }\n\n        this.percent = this.dxy.x / this.containerWidth;\n\n        let next;\n\n        if (this.percent > 0) {\n            next = this.mod(this.activeIndex - 1, this.slides.length)\n        }\n        else {\n            next = this.mod(this.activeIndex + 1, this.slides.length)\n        }\n\n        if (next != this.nextIndex) {\n\n            if (this.nextIndex != null) {\n                this.slides[this.nextIndex].htmlElement.style.opacity = 0;\n            }\n            this.nextIndex = next;\n            this.slides[this.nextIndex].htmlElement.style.opacity = 1;\n            if (this.pairity) this.slides[this.nextIndex].htmlElement.style.transform = this.rotateAxis + '(180deg) translateZ(3px)';\n            else this.slides[this.nextIndex].htmlElement.style.transform = this.rotateAxis + '(0deg) translateZ(3px)';\n        }\n        this.rotator.style.transform = this.rotateAxis + '(' + (this.lastRotation + this.percent * 180) + 'deg)';\n    }\n\n    this.handleUp = function () {\n        if (!this.interactionsStarted) return;\n        if (!this.preventedFlag) {\n            this.rotator.style.transition = 'transform .8s';\n            return;\n        }\n        // bntTracking.trackEvent(\"#User Interaction ended\", null);\n        this.preventedFlag = false;\n        this.interactionsStarted = false;\n        this.checkForChange();\n    }\n\n    this.checkForChange = function () {\n        this.rotator.style.transition = 'transform .8s';\n        this.lastRotation = this.lastRotation + Math.round(this.percent) * 180;\n        this.rotator.style.transform = this.rotateAxis + '(' + this.lastRotation + 'deg)';\n\n        if (Math.abs(this.percent) > 0.5) {\n            this.slides[this.activeIndex].htmlElement.style.opacity = 0;\n            this.slides[this.activeIndex].htmlElement.style.zIndex = '';\n            this.activeIndex = this.nextIndex;\n            this.slides[this.activeIndex].htmlElement.style.zIndex = '99';\n\n            this.trackSlidesOnce();\n\n            this.nextIndex = null;\n            this.pairity = !this.pairity;\n\n        }\n\n        //if current slide contains video, play it\n        if (this.activeIndex == this.videoidx) {\n            this.videoEl.play();\n            this.vidPlaying = true;\n        }\n        // if there is a potential video on a different slide, (would have been paused in handleDown), mark it as not playing\n        else if (this.activeIndex != this.videoidx) {\n            //has already been paused if it exists\n            this.vidPlaying = false;\n\n\n            // flag spam to prevent interaction for 500ms & add flip animation\n            this.spam = true;\n            setTimeout(function () {\n                this.spam = false;\n                this.container.htmlElement.classList.add(this.animation);\n            }.bind(this), 500)\n        }\n    }\n\n\n\n    this.addInteractions = function () {\n        this.events = pointerEvents();\n\n        this.container.htmlElement.addEventListener(this.events.click, function () {\n            this.interactionsStarted = false;\n        }.bind(this));\n        this.container.htmlElement.addEventListener(this.events.down, this.handleDown.bind(this), { capture: true });      // down\n        this.container.htmlElement.addEventListener(this.events.up, this.handleUp.bind(this), true);                       // up\n        this.container.htmlElement.addEventListener(this.events.move, this.handleMove.bind(this));                         // move        \n        this.container.htmlElement.addEventListener(this.events.cancel, this.handleUp.bind(this));                         // cancel\n    }\n\n    this.updatePercent = function () {\n        bnt.TeadsPlayerAddons.apiProxy.addObserver(function (api) {\n            if (api) {\n                api.getSlotBounds().map(function (slotBounds) {\n                    topWindowHeight = slotBounds.viewportHeight;\n                    playerTop = slotBounds.top;\n                    this.percent = 1 - playerTop / (topWindowHeight - window.innerHeight);\n                    this.percent = this.percent > 1 ? 1 : (this.percent < 0 ? 0 : this.percent);\n                }.bind(this));\n            }\n        }.bind(this))\n    }\n\n\n    this.trackSlidesOnce = function () {\n\n        if (this.trackedSlides.indexOf(this.activeIndex) < 0) {\n            bntTracking.trackEvent(\"#Slide \" + (this.activeIndex + 1) + ': in view', null);\n            this.trackedSlides.push(this.activeIndex);\n        }\n    }\n\n    this.loop = function () {\n        this.spinLoop = bnt.requestAnimFrame(this.loop.bind(this));\n        if (this.spam) return;\n\n        this.updatePercent();\n        this.updateToSlide = Math.floor(this.percent / (1 / (this.slides.length)));\n        if (this.updateToSlide >= this.slides.length) this.updateToSlide = this.slides.length - 1;\n\n        if (this.activeIndex === this.updateToSlide)\n            return;\n\n        this.rotateSlides();\n\n    };\n\n    this.stopLoop = function () {\n        bnt.cancelAnimFrame(this.spinLoop);\n    }\n\n    this.rotateSlides = function () {\n        this.spam = true;\n\n        let oldIndex = this.activeIndex;\n\n        this.nextIndex = this.updateToSlide;\n\n        if (this.pairity) this.slides[this.nextIndex].htmlElement.style.transform = this.rotateAxis + '(180deg) translateZ(3px)';\n        else this.slides[this.nextIndex].htmlElement.style.transform = this.rotateAxis + '(0deg) translateZ(3px)';\n\n        if (this.activeIndex > this.nextIndex) this.lastRotation = this.lastRotation + 180;\n        else this.lastRotation = this.lastRotation - 180;\n        this.rotator.style.transform = this.rotateAxis + '(' + this.lastRotation + 'deg)';\n\n        this.slides[this.nextIndex].htmlElement.style.opacity = 1;\n        this.slides[this.activeIndex].htmlElement.style.zIndex = '';\n        this.slides[this.nextIndex].htmlElement.style.zIndex = '99';\n\n        this.handleTransitionEnd(oldIndex);\n    }\n\n\n    this.handleTransitionEnd = function (oldIndex) {\n        this.transitionEndHandler = function (e) {\n\n            if (e.propertyName.toLowerCase() != \"transform\")\n                return;\n\n            this.slides[oldIndex].htmlElement.style.opacity = 0;\n            this.activeIndex = this.nextIndex;\n\n            this.trackSlidesOnce();\n\n            this.nextIndex = null;\n            this.pairity = !this.pairity;\n\n            if (this.activeIndex == this.videoidx) {\n                this.videoEl.play();\n                this.vidPlaying = true;\n\n                if (this.stayVideo) {\n                    this.stopLoop();\n                }\n            } else {\n                if (this.videoidx != null)\n                    this.videoEl.pause();\n                this.vidPlaying = false;\n            }\n\n            this.spam = false;\n\n            this.rotator.removeEventListener('transitionend', this.transitionEndHandler)\n\n        }.bind(this);\n\n        this.rotator.addEventListener('transitionend', this.transitionEndHandler)\n    }\n\n\n\n    this.autoNext = function () {\n        if (this.spam) return;\n        this.updateToSlide = this.activeIndex + 1;\n        if (this.updateToSlide >= this.slides.length) {\n            clearInterval(this.autoInterval);\n            this.container.htmlElement.classList.add(this.animation);\n            return;\n        };\n\n        if (this.activeIndex === this.updateToSlide) return;\n\n        this.rotateSlides();\n\n    }\n\n    this.mod = function (n, m) {\n        return ((n % m) + m) % m;\n    }\n}\n\nvar carousel = new flipCarousel(config);\n\nvar is_mobile = /(android|iphone|ipad)/gi.test(navigator.userAgent);\n\nfunction toXY(evt) {\n    var s = scale.get();\n\n    if (is_mobile) {\n        return {\n            x: evt.pageX = evt.touches[0].pageX * s,\n            y: evt.pageY = evt.touches[0].pageY * s,\n        }\n    } else {\n        return {\n            x: evt.pageX * s,\n            y: evt.pageY * s\n        }\n    }\n}\n\nfunction pointerEvents() {\n    if (is_mobile) {\n        return {\n            down: \"touchstart\",\n            up: \"touchend\",\n            move: \"touchmove\",\n            cancel: \"touchcancel\"\n        }\n    } else {\n        return {\n            down: \"mousedown\",\n            up: \"mouseup\",\n            move: \"mousemove\",\n            cancel: \"mouseleave\"\n        }\n    }\n}\n\nvar scale = new function () {\n    var scale = 1;\n\n    this.get = function () { return scale; };\n\n    function calculateScale() {\n        scale = creative.canvases[0].config.width / window.innerWidth;\n    }\n\n    calculateScale();\n\n    window.addEventListener(\"resize\", function () {\n        calculateScale();\n    });\n};\n\n\n/************************************************************\n* ==> Check if Display viewable\n************************************************************/\nif (!creative.screens[0].deepGetEosByType(bnt.Video).length) {\n    bnt.TeadsPlayerAddons.apiProxy.addObserver(function (api) {\n        if (api) {\n            api.getStudioData().map(function (data) {\n                if (data) data.display = true; else data = { display: true };\n                api.setStudioData(data).map(function () {\n                    var state = bnt.get(bnt.State);\n                    if (state) {\n                        api.sendVideoMetadata({ width: state.canvas.config.width, height: state.canvas.config.height });\n                    } else {\n                        var fixStage = function (state) {\n                            api.sendVideoMetadata({ width: state.canvas.config.width, height: state.canvas.config.height }); // force player to resize slot in case it got a different size from the vast tag\n                            bnt.get(bnt.StateChangeDetector).stateUpdated.removeObserver(fixStage); // we do this only once -\n                        };\n                        bnt.get(bnt.StateChangeDetector).stateUpdated.addObserver(fixStage);\n                    }\n                });\n            });\n        }\n        if (window.parent.adApi && window.parent.adApi.bntAd) {\n            window.parent.adApi.bntAd.environment.videoSlot = null;\n        }\n        bnt.TeadsPlayerAddons.brandingModeOnVoidClick = false;\n    });\n}\n\n\n//sound\n/******************************************\n * Sound Icon config\n *****************************************/\n\nvar soundConfig = {\n    left: '', //Position on X axis. Leave just the quotes for default\n    top: '', //Position on Y axis. Leave just the quotes for default\n    customIcon: '', //Name of the svg file goes between the quotes. Leave just the quotes if you don't wish to change sound color\n    screensToHideSoundIcon: [Screen1], //Name of the screens the sound icon should not be displayed; Separate them with comma. Leave it empty for default\n    videosToShowSoundIconOn: [MainVideo] //Name of the Video the sound icon should be displayed; Separate them with comma. Leave it empty for default\n}\n\n/******************************************\n * Sound Icon visibile calcs\n *****************************************/\nfunction createVisibleCss() {\n    var soundIconSize = 60;\n\n    var cssObj = {} //right, left, top, bottom, transform, display, padding\n\n    var scale = window.innerWidth / creative.canvases[0].config.width;\n\n    var leftTrimLen = soundConfig.left.trim().length;\n    var rightTrimLen = soundConfig.top.trim().length;\n\n    if (leftTrimLen) {\n        cssObj.right = `auto !important`\n        cssObj.left = `${soundConfig.left * scale}px !important`\n\n        cssObj.transform = `translateX(-50%) `\n        cssObj['-webkit-transform'] = `translateX(-50%) `\n    }\n\n    if (rightTrimLen) {\n        cssObj.bottom = `auto !important`;\n        cssObj.top = `${soundConfig.top * scale}px !important`;\n\n        if (cssObj.transform) cssObj.transform += `translateY(-50%)`;\n        else cssObj.transform += `translateY(-50%)`;\n\n        if (cssObj['-webkit-transform']) cssObj['-webkit-transform'] += `translateY(-50%)`;\n        else cssObj['-webkit-transform'] += `translateY(-50%)`;\n    }\n\n    if (leftTrimLen || rightTrimLen) {\n        cssObj.padding = `0px`\n\n        cssObj.width = `${soundIconSize * scale}px !important`;\n        cssObj.height = `${soundIconSize * scale}px !important`;\n\n        cssObj.display = 'flex !important';\n        cssObj['justify-content'] = 'center';\n        cssObj['align-items'] = 'center';\n    }\n\n    var cssStr = 'div.teads-ui-component-soundbutton {\\n';\n\n    Object.keys(cssObj).forEach(function (key) {\n        cssStr += key + \": \" + cssObj[key] + \";\\n\"\n    })\n\n    cssStr += '}'\n\n    return cssStr;\n}\n\n/******************************************\n * Sound Icon hidden calcs\n *****************************************/\nfunction createHiddenCss() {\n    return 'div.teads-ui-component-soundbutton {right: -2000px !important; }';\n}\n\n/******************************************\n * Sound Icon icon set\n *****************************************/\nfunction createVisualCss() {\n    if (soundConfig.customIcon.trim().length) {\n        return `.icon-muteoff, .icon-muteon {background-image: url( ${bnt.LIBRARY[soundConfig.customIcon].link}) !important;}`\n    } else return ''\n}\n\n/******************************************\n * Check Player Api and Update sound icon\n *****************************************/\nbnt.TeadsPlayerAddons.apiProxy.addObserver(function (api) {\n    if (api) {\n        creative.screens.forEach(function (screen) {\n            screen.onshow.addObserver(function () {\n                if (soundConfig.screensToHideSoundIcon.indexOf(screen) != -1) {\n                    bnt.TeadsPlayerAddons.callPlayerApiMethod('customizeUI', [createHiddenCss()])\n                } else {\n                    bnt.TeadsPlayerAddons.callPlayerApiMethod('customizeUI', [\n                        createVisibleCss() + createVisualCss()\n                    ]);\n                }\n            })\n        })\n\n        if (soundConfig.videosToShowSoundIconOn.length) {\n\n            soundConfig.videosToShowSoundIconOn.forEach(function (vid) {\n                vid.onplay.addObserver(function () {\n                    bnt.TeadsPlayerAddons.callPlayerApiMethod('customizeUI', [\n                        createVisibleCss() + createVisualCss()\n                    ]);\n                })\n                vid.onend.addObserver(function () {\n                    bnt.TeadsPlayerAddons.callPlayerApiMethod('customizeUI', [createHiddenCss()])\n                })\n                vid.onpause.addObserver(function () {\n                    bnt.TeadsPlayerAddons.callPlayerApiMethod('customizeUI', [createHiddenCss()])\n                })\n            })\n        }\n    }\n})\n\n}";

var bntAd = new adaptors.VpaidAd();
bntAd.setup(adsrc);

function getVPAIDAd() {return bntAd;}