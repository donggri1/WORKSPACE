package days06;

import java.util.Arrays;

public class Ex2 {

	public static void main(String[] args) {

		int a = 91;
		int b = 2;
		
		int ab= Integer.parseInt(a+""+b);
		int ab2 =2*a*b;
		
		int answer = Math.max(ab, ab2);
		if (ab==ab2) {
			answer = ab;
		}
		System.out.println(answer);
				


		
		


		
	}//main

}