package days06;

public class Ex2 {
	public static void main(String[] args) {

		String my_string = "string";

		int k =3;

		String answer =  solution(my_string, k);
		System.out.print(answer);
		
		for (int i = 1; i < k; i++) {
			
			my_string +=my_string;
			
		}

	}

	public static String solution(String my_string, int k) {
		String answer = "";
		
		for (int i = 0; i < k; i++) {
			System.out.print(my_string);
			
		}
		
		
		return answer;
	}

}