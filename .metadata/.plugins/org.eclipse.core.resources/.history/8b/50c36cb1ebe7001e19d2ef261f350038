package days05;

public class Ex03_02 {

	public static void main(String[] args) {
//		int currentPage = 1;
		int numberPerPage = 10;

		
		int start =1, end=10;
		for (int currentPage = 1; currentPage <= 5; currentPage++) {
						
			start = (currentPage-1)*numberPerPage+1;
			end = start +numberPerPage-1;
	
		}

		
		
	}//main

}//class
